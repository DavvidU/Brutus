@model List<Brutus.DTOs.UczenWithKonto>

<h1>Uczniowie</h1>

@if (Model != null)
{
    <table>
        <thead>
        <tr>
            <th>ID</th>
            <th>Imie</th>
            <th>Nazwisko</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var uczen in Model)
        {
            <tr>
                <td>@uczen.ID_Ucznia</td>
                <td>@uczen.Imie</td>
                <td>@uczen.Nazwisko</td>
                <!--<td>@uczen.ID_Rodzica</td>-->
            </tr>
        }
        </tbody>
    </table>
}
else
{
    <p>Brak dzieci do wy≈õwietlenia.</p>
}